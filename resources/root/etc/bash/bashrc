# /etc/bash/bashrc

# Do not edit this file.
# Place your readable configs in /etc/bash/bashrc.d/*.sh

if [[ $- != *i* ]]
then
  # Shell is non-interactive. Be done now!
  return
fi

if [ -d /etc/bash/bashrc.d/ ]
then
  for i in /etc/bash/bashrc.d/*.sh
  do
    if [ -r $i ]
    then
      . $i
    fi
  done
  unset i
fi
