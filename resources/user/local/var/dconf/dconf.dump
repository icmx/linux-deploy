[org/cinnamon/desktop/applications/terminal]
exec-arg='-e'
exec='urxvt'

[org/gtk/settings/file-chooser]
sort-column='name'
sidebar-width=156
window-position=(0, 0)
show-size-column=true
show-hidden=true
window-size=(757, 474)
sort-directories-first=false
date-format='regular'
sort-order='ascending'
location-mode='path-bar'

[org/virt-manager/virt-manager/vmlist-fields]
host-cpu-usage=true
network-traffic=false
disk-usage=false

[org/virt-manager/virt-manager/details]
show-toolbar=false

[org/nemo/compact-view]
all-columns-have-same-width=false

[org/nemo/preferences]
show-full-path-titles=false
show-search-icon-toolbar=true
show-advanced-permissions=true
show-home-icon-toolbar=true
show-new-folder-icon-toolbar=true
show-previous-icon-toolbar=true
show-compact-view-icon-toolbar=false
show-icon-view-icon-toolbar=true
show-list-view-icon-toolbar=true
show-up-icon-toolbar=true
show-open-in-terminal-toolbar=false
show-next-icon-toolbar=true
size-prefixes='base-2'
default-folder-viewer='list-view'
show-edit-icon-toolbar=false
date-format='iso'
show-hidden-files=true
show-image-thumbnails='never'
show-computer-icon-toolbar=false
show-reload-icon-toolbar=true
show-location-entry=true

[org/nemo/icon-view]
default-use-tighter-layout=true
labels-beside-icons=false

[org/nemo/list-view]
search-visible-columns=['name', 'size', 'type', 'where']

[org/nemo/sidebar-panels/tree]
show-only-directories=false
